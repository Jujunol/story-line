<% include ../partials/header-admin.ejs %>
<% include ../partials/sidebar.ejs %>

<div class="container-fluid col-sm-offset-1 col-sm-11" style="background: #FFF; border-top: 1px solid #e7e7e7; margin-top: 100px">
    <div class="col-sm-8 col-sm-offset-2">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3>Registered Admins</h3>
            </div>
            <div class="panel-body">
                Currently logged in as: <%= user.username %>
                <ul class="list-group">
                    
                    <% admins.forEach(function(admin) { %>
                        <li class="list-group-item">
                            <a href="/admin/<%= admin.username %>">
                                <%= admin.username %>
                            </a>
                            <span class="pull-right">
                                <%= admin.created.toDateString(); %>
                            </span>
                        </li>
                    <% }); %>
                    
                </ul>
            </div>
        </div>
    </div>
</div>

<% include ../partials/footer.ejs %>